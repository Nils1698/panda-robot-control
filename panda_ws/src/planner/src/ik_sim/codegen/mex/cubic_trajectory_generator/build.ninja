# CompilerName=mingw64
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/win64
matlabdir = F:/MATLAB/R2021b
startdir = F:/DTU/M.Sc/5th_win_msc/program/inverse_kinematics_sim

# Toolchain information
cc = C:\ProgramData\MATLAB\SupportPackages\R2021b\3P.instrset\mingw_w64.instrset\bin\gcc
cxx = C:\ProgramData\MATLAB\SupportPackages\R2021b\3P.instrset\mingw_w64.instrset\bin\g++
linker = C:\ProgramData\MATLAB\SupportPackages\R2021b\3P.instrset\mingw_w64.instrset\bin\gcc
linkerxx = C:\ProgramData\MATLAB\SupportPackages\R2021b\3P.instrset\mingw_w64.instrset\bin\g++

cflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c99 -I "F:\DTU\M.Sc\5th_win_msc\program\inverse_kinematics_sim\codegen\mex\cubic_trajectory_generator" -I "F:\DTU\M.Sc\5th_win_msc\program\inverse_kinematics_sim" -I "F:\DTU\M.Sc\5th_win_msc\program\inverse_kinematics_sim\codegen\mex\cubic_trajectory_generator\interface" -I "F:\MATLAB\R2021b\extern\include" -I "." -DMODEL=cubic_trajectory_generator_mex -D__USE_MINGW_ANSI_STDIO=1 -DMW_NEEDS_VERSION_H  

cxxflags =  -c -fexceptions -fno-omit-frame-pointer -std=c++11 -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "F:\DTU\M.Sc\5th_win_msc\program\inverse_kinematics_sim\codegen\mex\cubic_trajectory_generator" -I "F:\DTU\M.Sc\5th_win_msc\program\inverse_kinematics_sim" -I "F:\DTU\M.Sc\5th_win_msc\program\inverse_kinematics_sim\codegen\mex\cubic_trajectory_generator\interface" -I "F:\MATLAB\R2021b\extern\include" -I "." -DMODEL=cubic_trajectory_generator_mex -D__USE_MINGW_ANSI_STDIO=1 -DMW_NEEDS_VERSION_H  

ldflags =  -m64 -Wl,--no-undefined -shared -static -L"F:\MATLAB\R2021b\extern\lib\win64\mingw64" -llibmx -llibmex -llibmat -lm -llibmwlapack -llibmwblas -llibMatlabDataArray -llibMatlabEngine  -o "cubic_trajectory_generator_mex.mexw64" -llibemlrt -llibcovrt -llibut -llibmwmathutil 

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker @$out.rsp $ldflags
    rspfile = $out.rsp
    rspfile_content = $in

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/cubic_trajectory_generator_data.o : cxx $root/cubic_trajectory_generator_data.cpp
build $builddir/rt_nonfinite.o : cxx $root/rt_nonfinite.cpp
build $builddir/cubic_trajectory_generator_initialize.o : cxx $root/cubic_trajectory_generator_initialize.cpp
build $builddir/cubic_trajectory_generator_terminate.o : cxx $root/cubic_trajectory_generator_terminate.cpp
build $builddir/cubic_trajectory_generator.o : cxx $root/cubic_trajectory_generator.cpp
build $builddir/_coder_cubic_trajectory_generator_api.o : cxx $root/interface/_coder_cubic_trajectory_generator_api.cpp
build $builddir/_coder_cubic_trajectory_generator_mex.o : cxx $root/interface/_coder_cubic_trajectory_generator_mex.cpp
build $builddir/_coder_cubic_trajectory_generator_info.o : cxx $root/interface/_coder_cubic_trajectory_generator_info.cpp
build $builddir/cpp_mexapi_version.o : cxx $matlabdir/extern/version/cpp_mexapi_version.cpp

# Link
build $root\cubic_trajectory_generator_mex.mexw64 : linkxx $builddir/cubic_trajectory_generator_data.o $builddir/rt_nonfinite.o $builddir/cubic_trajectory_generator_initialize.o $builddir/cubic_trajectory_generator_terminate.o $builddir/cubic_trajectory_generator.o $builddir/_coder_cubic_trajectory_generator_api.o $builddir/_coder_cubic_trajectory_generator_mex.o $builddir/_coder_cubic_trajectory_generator_info.o $builddir/cpp_mexapi_version.o 
